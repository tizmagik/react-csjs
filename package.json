{
  "name": "react-csjs",
  "version": "2.0.0",
  "description": "React Higher Order Component for CSJS",
  "main": "lib",
  "scripts": {
    "build": "cross-env NODE_ENV=production BABEL_ENV=commonjs babel src --out-dir lib",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha",
    "prepublish": "npm run lint && npm run test && npm run build",
    "lint": "eslint ./src ./test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tizmagik/react-csjs.git"
  },
  "keywords": [
    "react",
    "csjs",
    "css",
    "style",
    "stylesheet",
    "hoc",
    "decorator"
  ],
  "peerDependencies": {
    "csjs": ">=1.0.0 < 5.0.0",
    "react": ">=0.13"
  },
  "author": "Jeremy Gayed (https://github.com/tizmagik)",
  "contributors": [
    "Paul Henschel <drcmda@gmail.com>",
    "Justin Hall <justin.r.hall@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tizmagik/react-csjs/issues"
  },
  "homepage": "https://github.com/tizmagik/react-csjs#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.0",
    "babel-plugin-istanbul": "^2.0.3",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "babel-template": "^6.16.0",
    "babel-types": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "csjs": "^1.0.5",
    "enzyme": "^2.6.0",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.6.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.1",
    "nyc": "^8.3.1",
    "react": "^15.2.1",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.2.1",
    "sinon": "^1.17.6",
    "source-map-support": "^0.4.6"
